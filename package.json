{
  "name": "ynab-counter",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "build": "pnpm -C apps/web build",
    "start": "pnpm -C apps/web start",
    "typecheck": "pnpm -r --parallel typecheck",
    "lint": "eslint .",
    "db:generate": "pnpm -C packages/db generate",
    "db:migrate": "pnpm -C packages/db migrate:dev"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "docs"
  ],
  "devDependencies": {
    "typescript": "5.6.2"
  }
}
